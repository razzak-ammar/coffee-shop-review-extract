// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../reviews.db"
}

model Review {
  id       Int     @id @default(autoincrement())
  title   String  
  review  String
}

model Keyword {
  id      Int     @id @default(autoincrement())
  phrase  String
}